<script>
	// jQuery script to generate a QR-code to the current URL
	$(document).ready(function(){
		$(".qrspace").append($('<div/>',{
			id:'qrCode'
		}));
		$('#qrCode').qrcode({
			render: "canvas", // or 'table'
			width:100,
			height:100,
			background: "#ffffff",
			foreground: "#000000",
			text:window.location.href
		});
	});
</script>
