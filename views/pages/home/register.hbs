<body id="menu" class="body-page">

	<!-- Page Loader animation : just comment these lines to remove it -->
  <div class="page-loader" id="page-loader">
    <div>
      <div class="icon ion-spin"></div>
      <p>loading</p>
    </div>
  </div>

  {{>navbar-main btn='hidden'}}
    </div>
    <!-- end of menu wrapper -->

  </header>
  <!-- END OF site header Menu-->

	<div class='formbg'>

            	<br>
			<!--BEGINNING of Form-->
       	<form action="/register" method="POST" class="form-box" >

				<!-- title -->
				<div id="regformheader">
						Application for Startup Incubation in<br>
						INNOVATION AND ENTREPRENEURSHIP DEVELOPMENT CELL<br>
						NSS College of Engineering, Palakkad
				</div>
				<!--End of title-->	
	
       		<!--Section 1 Applicant details-->
            <div class="well"><i class="fa fa-user"></i> Applicant</div>
                
  		<div class="row formpad">
			<div class="form-group col-md-3"><label for="firstname">Full name:<font color=red>*</font></label>
    			<input type="text" name="firstname" id="firstname" class="form-control-line form-control-white" required placeholder="First name" />
			</div>
			<div class="form-group col-md-3"><label for="lastname">..<font color=red>*</font></label></label>
    			<input type="text" name="lastname" id="lastname" class="form-control-line form-control-white" required placeholder="Last name" />
			</div>
			<div class="form-group col-md-3"><label for="semester">Semester:<font color=red>*</font></label>
    			<select  name="semester" id="semester" class="form-control-line form-control-white" required />
    			<option value="Sem1">Semester 1</option>
    			<option value="Sem2">Semester 2</option>
    			<option value="Sem3">Semester 3</option>
    			<option value="Sem4">Semester 4</option>
    			<option value="Sem5">Semester 5</option>
    			<option value="Sem6">Semester 6</option>
    			<option value="Sem7">Semester 7</option>
    			<option value="Sem8">Semester 8</option>
				</select>
			</div>
			<div class="form-group col-md-3"><label for=branch>Branch<font color=red>*</font></label>
				<select name="branch" id="branch" class="form-control-line form-control-white" required>
				<option value="Civil">Civil Engineering</option>
    			<option value="Computer Science">Computer Science Engineering</option>
    			<option value="Electrical and Electronics">Electrical & Electronics Engineering</option>
    			<option value="Electronics and Communication">Electronics and Communication Engineering</option>
       			<option value="Instrumentation and Control">Instrumentation & Control Engineering</option>
    			<option value="Mechanical">Mechanical Engineering</option>
				</select>
			</div>
			<div class="form-group col-md-3"><label for="Email">Email:<font color=red>*</font></label>
    			<input type="Email" name="email" id="email" class="form-control-line form-control-white" required placeholder="yourid@host.domain" />
			</div>
			<div class="form-group col-md-3"><label for="mobile">Mobile:<font color=red>*</font></label>
    			<input type="number" name="mobile" id="mobile" class="form-control-line form-control-white" required placeholder="Mobile number" />
			</div>
		</div>
		<!--End of section 1-->

		<!--Section 2 Cofounder(s) Details-->

		<div class="well" ><i class="fa fa-users"></i>&nbsp;Co Founder(s)
		
		</div>
                
  		<div class="row formpad">
			<div class="form-group col-md-3"><label for="cf1name">Name of Co Founder 1:<font color=red>*</font></label>
    			<input type="text" name="cf1name" id="cf1name" class="form-control-line form-control-white" required placeholder="First name" />
			</div>
			<div class="form-group col-md-3"><label for="cf1lname">..<font color=red>*</font></label></label>
    			<input type="text" name="cf1lname" id="cf1lname" class="form-control-line form-control-white" required placeholder="Last name" />
			</div>
			<div class="form-group col-md-3"><label for="cf1sem">Semester:<font color=red>*</font></label>
    			<select  name="cf1sem" id="cf1sem" class="form-control-line form-control-white" required />
    			<option value="sem1">Semester 1</option>
    			<option value="sem2">Semester 2</option>
    			<option value="sem3">Semester 3</option>
    			<option value="sem4">Semester 4</option>
    			<option value="sem5">Semester 5</option>
    			<option value="sem6">Semester 6</option>
    			<option value="sem7">Semester 7</option>
    			<option value="sem8">Semester 8</option>
				</select>
			</div>
			<div class="form-group col-md-3"><label for="cf1branch">Branch<font color=red>*</font></label>
				<select name="cf1branch" id="cf1branch" class="form-control-line form-control-white" required>
				<option value=Civil>Civil Engineering</option>
    			<option value=Computer>Computer Science Engineering</option>
    			<option value=Electrical>Electrical & Electronics Engineering</option>
    			<option value=Electronics>Electronics and Communication Engineering</option>
       			<option value=Instrumentation>Instrumentation & Control Engineering</option>
    			<option value=Mechanical>Mechanical Engineering</option>
				</select>
				
			</div>
			<div class="form-group col-md-3"><label for="Email1">Email:<font color=red>*</font></label>
    			<input type="Email" name="Email1" id="Email1" class="form-control-line form-control-white" required placeholder="yourid@host.domain" />
			</div>
			<div class="form-group col-md-3"><label for="mobile1">Mobile:<font color=red>*</font></label>
    			<input type="number" name="mobile1" id="mobile1" class="form-control-line form-control-white" required placeholder="Mobile number" />
			</div>
			<div class="form-group col-md-3">Is Your CoFounder a NSSCE student
				<div class=row>
				<div class="col-md-6">
				<label for name="NSSIAN1">Yes<input type="radio" name="NSSIAN1" id="NSSIAN1" class="form-control-inline" value="Yes" required></label></div>
				<div class="col-md-6">
				<label for name="NSSIAN1">No<input type="radio" name="NSSIAN1"  id="NSSIAN1" class="form-control-inline" value="No" required></label></div>
				</div>
			</div>
		</div>
		<!-- Add another cofounder button code -->
		<div class="input_fields_wrap">
		<div class="btns-action anim-3" align="right">
				        
		<button class="add_field_button btn btn-outline-white btn-round"><i class="fa fa-user-plus"></i>&nbsp;Add Another Co Founder</button>
			</div>
			</div>
		<!--End of section 2-->

        <!--Section 3 Business details-->
        <div class="well" ><i class="fa fa-institution"></i>&nbsp;Business</div>
                
  		<div class="row formpad">
			<div class="form-group col-md-6" ><label for="company">Company Name:<font color=red>*</font></label>
    			<input type="text" name="company" id="company" class="form-control-line form-control-white" required placeholder="Company Name" />
			</div>
			<div class="form-group col-md-6" ><label for="website">Website<font color=red>*</font></label>
    			<input type="text" name="website" id="website" class="form-control-line form-control-white" required placeholder="Website Address" />
			</div>
			<div class="form-group col-md-6"><label for="employees">Do you have Employees? If yes, how many?<font color=red>*</font></label>
    			<input type="text" name="employee" id="employee" class="form-control-line form-control-white" required placeholder="Employee Details" />
			</div>
		</div>
		<!--End of section 3-->
		
        <!--Section 4 Business details-->
         <div class="well" ><i class="fa fa-bar-chart"></i>&nbsp;Business Description</div>
                
  		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="desc">Please provide a brief description of your business idea<font color=red>*</font></label>
    			<textarea name="desc" id="desc" class="form-control-line form-control-white" required maxlength="3000" placeholder="Describe your Idea" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="whopay">Key customer Segments.Who are the People or the Companies who will pay for the Product/Service?<font color=red>*</font></label>
    			<textarea name="whopay" id="whopay" class="form-control-line form-control-white" required maxlength="3000" placeholder="Describe your customer base" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="custpain">Define the Customer's pain, problem or unmet need.<font color=red>*</font></label>
    			<textarea name="custpain" id="custpain" class="form-control-line form-control-white" required /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="Competitors">Key Competitors/How your customer solve this problem today?<font color=red>*</font></label>
    			<textarea name="competitors" id="competitors" class="form-control-line form-control-white" required maxlength="3000" placeholder="Your product/Service Competitors" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="UVP">Unique Value Proposition(How will your solution be better than the Competition?)<font color=red>*</font></label>
    			<textarea name="UVP" id="UVP" class="form-control-line form-control-white" required maxlength="3000" placeholder="Unique/Advanced feature of yor product" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="solution">Solution (How will you solve the problem for the customer?)<font color=red>*</font></label>
    			<textarea name="solution" id="solution" class="form-control-line form-control-white" required maxlength="3000" placeholder="Describe your Idea" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="copyright">Is there any intellectual property(trade secrets, copyrights,patents etc.) potentially associated with your idea?<font color=red>*</font></label>
    			<textarea name="copyright" id="copyright" class="form-control-line form-control-white" required maxlength="3000" placeholder="Discuss potential patents" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="customers">What are the potential number of customers that could be interested in purchasing your product/service, and/or information you found on market size and growth for this field<font color=red>*</font></label>
    			<textarea name="customers" id="customers" class="form-control-line form-control-white" required maxlength="3000" placeholder="Potential number of customers" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="marketing">How will you sell your product/service?(Distribution, Sales channels, etc.)<font color=red>*</font></label>
    			<textarea name="marketing" id="marketing" class="form-control-line form-control-white" required maxlength="3000" placeholder="Your Marketing idea" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="makemoney">Revenue streams (How will you make money?)<font color=red>*</font></label>
    			<textarea name="makemoney" id="makemoney" class="form-control-line form-control-white" required maxlength="3000" placeholder="How will you make money" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="investment">Please Estimate your Business startup costs<font color=red>*</font></label>
    			<textarea name="investment" id="investment" class="form-control-line form-control-white" required maxlength="3000" placeholder="Initial investment costs for your startup" /></textarea>
			</div>
		</div>
		<hr>
		<div class="row formpad">
			<div class="form-group col-md-12" ><label for="Goals">How will you reach your financial goals for startup? (family, friends, loan, investors, etc.)<font color=red>*</font></label>
    			<textarea name="goals" id="goals" class="form-control-line form-control-white" required maxlength="3000" placeholder="Describe your Idea" /></textarea>
			</div>
		</div>
		<hr>
		<div class="form-group col-md-3"><label for="mentor">Please Select you Mentor:<font color="red">*</font></label>
    			<select  name="mentor" id="mentor" class="form-control-line form-control-white" required />
    			<option value="Surya S S">Surya S S (CIVIL DEPT)</option>
    			<option value="Kiran V K">Kiran V K (CSE DEPT)</option>
    			<option value="Jyothish Krishna">Jyothish Krishna (ECE DEPT)</option>
    			<option value="Vaishak T">Vaishak T (EEE DEPT)</option>
    			<option value="Nikhil Binoy C">Nikhil Binoy C (ICE DEPT)</option>
    			<option value="Kiron K R">Kiron K R (MECH DEPT)</option>
				</select>
		</div>
		<br>
		<div class="form-group col-md-12"  align="center">
		<input type="submit" class="btn btn-outline-white btn-round btns-action anim-3" id="formbtn" value="Submit" name="Submit">
		</div>
		  
		</form>
		<!--END of Form-->
               
	<br><br>        
	</div>

</div>

<!-- scripts -->
  <!-- All Javascript plugins goes here -->
  <script src="/assets/js/vendor/jquery-1.12.4.min.js"></script> 

  <!-- All vendor scripts -->

  <script src="/assets/js/vendor/all.js"></script>

