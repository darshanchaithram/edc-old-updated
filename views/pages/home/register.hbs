<style type="text/css">
	body{
		overflow-x: hidden;
	}
	select > option{
		background-color: rgba(0, 96, 100, 0.8)!important;
	}
	header,.all-menu-wrapper{
		background-color: rgba(0, 96, 100, 2.9)!important;
	}
	.navbar-mainmenu{
		background-color: rgba(0,96,100,0.8)!important;
	}

	.formbg{
		padding-left: 20;
		padding-right: 20;
	}
	.formpad{
		padding: 10px;
	}
	#formbtn:hover{
		box-shadow: 0 0 10px grey;
		background-color: #f1f1f1;
		color: #424242;
		font-weight: bold;
	}
	#regformheader{
		font-size: 2rem;
		padding-left: 10px;
